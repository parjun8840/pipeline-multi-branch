pipeline {
    agent {label 'DONT-USE-THIS'}
    stages {
        stage('Build Main') {
            when {
                branch 'main'
            }
            steps {
                echo 'Building main'
                script{
                foo()
        }
        }
        stage('Build Dev') {
            when {
                branch 'dev'
            }
            steps {
                echo 'Building dev'
            }
        }
    }
}

}
def foo(){
  list friends = ['A', 'B', 'C', 'D']
  for(item in friends) {
    println item
}
}

